    "def replace(str,char1,char2):\n",
    "    l = len(str)\n",
    "    newstr = \"\"\n",
    "    for i in range(l):\n",
    "        if str[i] == char1:\n",
    "            newstr = newstr + char2\n",
    "        else:\n",
    "            newstr = newstr + str[i]\n",
    "    return newstr\n",
    "\n",
    "\n",
    "str = \"fazlullah\"\n",
    "result = replace(str,\"l\",\"@\")\n",
    "print(result)"
